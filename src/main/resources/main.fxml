<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.TextFlow?>
<VBox id="groupChat" fx:id="groupChat" alignment="CENTER" prefHeight="464.0" prefWidth="664.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tf.chat24.MainController">
    <children>
        <HBox>
            <TabPane id="TabPane" fx:id="TabPane" prefHeight="600.0" prefWidth="600.0">
                <tabs>
                    <Tab id="ChatTab" fx:id="ChatTab" closable="false" text="Общий чат">
                        <content>
                            <VBox id="defVbox" prefHeight="439.0" prefWidth="400.0">
                                <children>
                                    <HBox spacing="10.0">
                                        <TextFlow id="textFlow" fx:id="textFlow" disable="true"  prefHeight="400.0" prefWidth="496.0" />
                                    </HBox>
                                    <HBox spacing="10.0">
                                        <TextField id="inputField" fx:id="inputField" prefColumnCount="30" prefHeight="25.0" prefWidth="437.0" />
                                        <Button id="enterButton" fx:id="enterButton" text="Enter" />
                                    </HBox>
                                </children>
                            </VBox>
                        </content>
                    </Tab>
                </tabs>
            </TabPane>
            <ListView id="userList" fx:id="userList" prefHeight="464.0" prefWidth="281.0" />
        </HBox>
    </children>
</VBox>
